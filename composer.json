{
  "name": "gigya/php-sdk",
  "description": "PHP SDK for Gigya",
  "type": "library",
  "version": "3.0.2",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.0",
    "firebase/php-jwt": "^5.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "Gigya\\PHP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
